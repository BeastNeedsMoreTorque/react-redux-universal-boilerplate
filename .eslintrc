{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "__CLIENT__": false,
    "__SERVER__": false,
    "__DEV__": false,
    "__PROD__": false,
    "__DEBUG__": false,
    "webpackIsomorphicTools": false
  },
  "rules": {
    "semi" : [2, "never"],
    comma-dangle: [1, "never"]
  }
}
